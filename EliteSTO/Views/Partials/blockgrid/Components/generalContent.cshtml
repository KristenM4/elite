@inherits UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockGridItem<ContentModels.GeneralContent>>

@if(Model.Content.Page?.ToLower().StripWhitespace() == "homepage")
{
    @await Html.PartialAsync("~/Views/Partials/HomePage/homepageContent.cshtml")
}
@if (Model.Content.Page?.ToLower().StripWhitespace() == "solution")
{
    @await Html.PartialAsync("~/Views/Partials/SolutionPage/solutionPageGeneralContent.cshtml")
}
@if (Model.Content.Page?.ToLower().StripWhitespace() == "about")
{
    @await Html.PartialAsync("~/Views/Partials/AboutPage/aboutPageGeneralContent.cshtml")
}