@inherits UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockGridItem<ContentModels.GeneralContent>>

@if(Model.Content.Section == 3)
{
    <div class="fade-in-section container padding-y-xl container-w-lg color-white">
        <div style="padding: 0px 20px 80px 20px;" class="flex-row-lg gap-3xl">
            <div class="content-center">@Model.Content.Title</div>
            <div class="underline-links">@Model.Content.Text</div>
        </div>
    </div>
}